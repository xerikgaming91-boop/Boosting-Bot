{
  "name": "boosting-bot",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -n BACKEND,FRONTEND,STUDIO -c blue,green,magenta \"node src/backend/server.js\" \"vite frontend\" \"prisma studio --schema src/backend/prisma/schema.prisma --port 5555\"",
    "build": "vite build",
    "prisma:generate": "prisma generate --schema src/backend/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema src/backend/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema src/backend/prisma/schema.prisma --port 5555",
    "bot:selftest": "node src/backend/discord/selftest.mjs"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.8"
  }
}
